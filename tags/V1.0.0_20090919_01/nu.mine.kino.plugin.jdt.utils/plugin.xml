<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="nu.mine.kino.plugin.jdt.utils.handlers.AddToStringHandler"
            commandId="nu.mine.kino.plugin.jdt.utils.commands.AddToStringCommand">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="nu.mine.kino.plugin.jdt.utils.commands.AddToStringCommand"
            name="%command.name">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="popup:org.eclipse.ui.popup.any">
         <command
               commandId="nu.mine.kino.plugin.jdt.utils.commands.AddToStringCommand"
               style="push">
         </command>
      </menuContribution>
   </extension>

</plugin>
